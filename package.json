  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-register": "^6.23.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.2",
    "eslint": "^3.16.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^3.0.0",
    "express": "^4.14.1",
    "npm-run-all": "^4.0.2",
    "nsp": "^2.6.2",
    "open": "0.0.5",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1"
  }
}
